    <script>




        $(document).ready(function () {
            $("#main-nav a").on('click', function () {

                //alert(this.hash);

                $("body").animate(
                    {
                        scrollTop:
                            $(this.hash).offset().top
                    },
                    1000
                )
            });
        });

    </script>

